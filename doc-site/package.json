{
  "name": "doc-site",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "vite serve",
    "debug": "node --inspect ./node_modules/vite/bin/vite serve",
    "build": "rimraf dist && vite build --outDir dist && serve -s dist",
    "ssr": "rimraf dist && vite-pages ssr",
    "ssr-for-preview": "rimraf dist && vite-pages ssr && serve dist",
    "ssr-for-deploy": "rimraf dist && cross-env GITHUB_PAGES_DEPLOY=true vite-pages ssr",
    "deploy": "npm run ssr-for-deploy && touch ./dist/.nojekyll && gh-pages -d dist -t"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.25",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^5.0.0",
    "rimraf": "^4.4.1",
    "serve": "^14.2.0",
    "vite": "^4.3.5",
    "@aliyun-sls/vite-pages-theme-doc": "workspace:*",
    "@aliyun-sls/vite-plugin-react-pages": "workspace:*"
  }
}
